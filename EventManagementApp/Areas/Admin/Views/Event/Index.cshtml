<h1>Events</h1>

<div id="eventPage" v-cloak>
    <form v-on:submit.prevent="onSubmit">
         <div>
            <label  for="email" class="form-label">Your email</label>
            <input v-model="form.account.email"  type="email" name="email" id="email" class="form-input" placeholder="name@company.com" >
            
        </div>
          {{errors?.age?.[0]}}
      
         <button class="btn-primary"  type="submit">Submit</button>
    </form>
    <div>
            {{form.email}}
    </div>

</div>

@section Scripts{
    <script src="~/dist/js/event/event-page.js"> </script>
}